{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Terraform Provider",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "env": {
        "FABRIC_PREVIEW": "true",
        "FABRIC_SDK_GO_LOGGING": "trace",
        "FABRIC_SDK_GO_LOGGING_INCLUDE_BODY": "true"
      },
      "args": [
        "-debug"
      ]
    }
  ]
}
